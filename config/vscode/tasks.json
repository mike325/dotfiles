{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "(Default g++17) Build",
            "command": "g++",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "${file}",
                "-o",
                "${fileDirname}/${fileBasenameNoExtension}",
                "-std=c++17",
                "-O2",
                "-Wall",
                "-Wextra",
                "-Wshadow",
                "-Wnon-virtual-dtor",
                "-Wold-style-cast",
                "-Wcast-align",
                "-Wunused",
                "-Woverloaded-virtual",
                "-Wpedantic",
                "-Wconversion",
                "-Wsign-conversion",
                "-Wnull-dereference",
                "-Wdouble-promotion",
                "-Wformat=2",
                "-ffunction-sections",
                "-fdata-sections"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build"
            },
            "detail": "Task generated by Debugger."
        },
        {
