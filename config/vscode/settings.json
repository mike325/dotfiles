{
    "editor.renderWhitespace": "trailing",
    "editor.fontFamily": "FiraCode NF, Menlo, Monaco, 'Courier New', monospace",
    "terminal.external.osxExec": "kitty.app",
    "telemetry.telemetryLevel": "crash",
    "C_Cpp.default.compilerPath": "clang++",
    "C_Cpp.loggingLevel": "Warning",
    "C_Cpp.clang_format_sortIncludes": true,
    "C_Cpp.autocompleteAddParentheses": true,
    "C_Cpp.clang_format_style": "Google",
    "C_Cpp.default.cppStandard": "c++17",
    "C_Cpp.default.cStandard": "c17",
    "C_Cpp.default.mergeConfigurations": true,
    "C_Cpp.experimentalFeatures": "Enabled",
    "C_Cpp.intelliSenseEngineFallback": "Enabled",
    "C_Cpp.vcFormat.newLine.beforeElse": false,
    "C_Cpp.vcFormat.newLine.beforeOpenBrace.block": "sameLine",
    "C_Cpp.vcFormat.newLine.beforeOpenBrace.function": "sameLine",
    "C_Cpp.vcFormat.newLine.beforeOpenBrace.lambda": "sameLine",
    "C_Cpp.vcFormat.newLine.beforeOpenBrace.namespace": "sameLine",
    "C_Cpp.vcFormat.newLine.beforeOpenBrace.type": "sameLine",
    "C_Cpp.vcFormat.newLine.beforeWhileInDoWhile": true,
    "C_Cpp.workspaceSymbols": "All",
    "C_Cpp.default.intelliSenseMode": "windows-clang-x64",
    "git.alwaysSignOff": true,
    "git.autofetch": true,
    "git.autoStash": true,
    "git.enableCommitSigning": true,
    "git.showPushSuccessNotification": true,
    "markdown.preview.fontSize": 13,
    "python.defaultInterpreterPath": "C:\\Users\\migochoa\\AppData\\Local\\Programs\\Python\\Python310\\python.exe",
    "rust-client.engine": "rust-analyzer",
    "latex-workshop.bibtex-fields.sort.enabled": true,
    "latex-workshop.bibtex-format.tab": "4 spaces",
    "latex-workshop.chktex.enabled": true,
    "latex-workshop.intellisense.update.aggressive.enabled": true,
    "latex-workshop.intellisense.unimathsymbols.enabled": true,
    "latex-workshop.latex.autoClean.run": "onFailed",
    "latex-workshop.view.pdf.viewer": "external",
    "C_Cpp.default.compilerArgs": [
        "-std=c++17",
        "-Wall",
        "-Wextra",
        "-Wshadow",
        "-Wnon-virtual-dtor",
        "-Wold-style-cast",
        "-Wcast-align",
        "-Wunused",
        "-Woverloaded-virtual",
        "-Wpedantic",
        "-Wconversion",
        "-Wsign-conversion",
        "-Wnull-dereference",
        "-Wdouble-promotion",
        "-Wformat=2",
        "-ffunction-sections",
        "-fdata-sections"
    ],
    "launch": {
        "version": "0.2.0",
        "configurations": [
          {
            "name": "C/C++: clang++ build and debug active file",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}\\${fileBasenameNoExtension}.exe",
            "args": [],
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "lldb",
            "miDebuggerPath": "C:\\Program Files\\LLVM\\bin\\lldb.exe",
            "preLaunchTask": "C/C++: clang++ build and debug active file"
          },
          {
            "name": "C/C++: clang++ build and run active file",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}\\${fileBasenameNoExtension}.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "preLaunchTask": "C/C++: clang++ build active file"
          },
          {
            "name": "C/C++: g++ build and debug active file",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}\\${fileBasenameNoExtension}.exe",
            "args": [],
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "C:\\Program Files\\mingw-w64\\x86_64-8.1.0-posix-seh-rt_v6-rev0\\mingw64\\bin\\gdb.exe",
            "setupCommands": [
              {
                "description": "Enable pretty-printing for gdb",
                "text": "-enable-pretty-printing",
                "ignoreFailures": true
              }
            ],
            "preLaunchTask": "C/C++: g++ build and run active file"
          },
          {
            "name": "C/C++: g++ build and run active file",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}\\${fileBasenameNoExtension}.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "preLaunchTask": "C/C++: g++ build active file"
          }
        ]
    }
}
