        {
            "command": "togglePaneZoom",
            "keys": "ctrl+shift+z"
        },
        {
            "command":
            {
                "action": "copy",
                "singleLine": false
            },
            "keys": "ctrl+shift+c"
        },
        {
            "command":
            {
                "action": "splitPane",
                "split": "auto",
                "splitMode": "duplicate"
            },
            "keys": "ctrl+shift+period"
        },
        {
            "command": "find",
            "keys": "ctrl+shift+f"
        },
        {
            "command": "paste",
            "keys": "ctrl+shift+v"
        },
        {
            "command":
            {
                "action": "closeTab"
            },
            "keys": "ctrl+alt+x"
        },
        {
            "command": "closePane",
            "keys": "ctrl+shift+w"
        }
    ],
